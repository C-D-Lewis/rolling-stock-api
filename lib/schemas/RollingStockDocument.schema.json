{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "required": [
    "type",
    "class",
    "unitNumber"
  ],
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "diesel",
        "electric",
        "passenger",
        "freight",
        "maintenance"
      ]
    },
    "class": {
      "type": "string"
    },
    "unitNumber": {
      "type": "string"
    }
  }
}